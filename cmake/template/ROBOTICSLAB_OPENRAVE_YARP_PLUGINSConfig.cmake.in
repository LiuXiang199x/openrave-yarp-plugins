# Once you find_package(ROBOTICSLAB_OPENRAVE_YARP_PLUGINS), it defines:
#
# * several targets; it's advised to link against individual libraries with the usual
#   target_link_libraries() command
#
# * ROBOTICSLAB_OPENRAVE_YARP_PLUGINS_MODULE_PATH; use list(APPEND CMAKE_MODULE_PATH ${ROBOTICSLAB_OPENRAVE_YARP_PLUGINS_MODULE_PATH})
#
# Notes:
#
# * All target names are prepended with the ROBOTICSLAB:: namespace

@PACKAGE_INIT@

set(ROBOTICSLAB_OPENRAVE_YARP_PLUGINS_MODULE_PATH "@PACKAGE_ROBOTICSLAB_OPENRAVE_YARP_PLUGINS_MODULE_DIR@")

list(APPEND ROBOTICSLAB_OPENRAVE_YARP_PLUGINS_MODULE_PATH ${ROBOTICSLAB_OPENRAVE_YARP_PLUGINS_MODULE_PATH}/modules)

if(NOT "@_exported_targets@" STREQUAL "")
    include(${CMAKE_CURRENT_LIST_DIR}/ROBOTICSLAB_OPENRAVE_YARP_PLUGINSTargets.cmake)
endif()
